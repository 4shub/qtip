    "url": "https://github.com/4shub/qtip/issues"
  },
  "homepage": "https://github.com/4shub/qtip#readme",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/mongodb": "^3.5.14",
    "@types/node": "^13.13.4",
    "@types/showdown": "^1.9.3",
    "node-sass": "^4.14.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
