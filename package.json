{
  "name": "qtip",
  "version": "0.1.0",
  "description": "A simple file hosting ui ",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "nodemon",
    "build:web": "mkdir -p src/cli/public && parcel build src/cli/actions/preview/preview.ts &&  cp dist/preview.js src/cli/public/preview.js && cp dist/preview.js public/root/preview.js && cp public/root/root.css src/cli/public/root.css",
    "build": "rimraf ./dist && rimraf ./public && tsc && mkdir ./public && npm run sass && cp src/server/root/root.html ./dist/server/root/root.html && npm run build:web",
    "test": "yarn test",
    "sass": "node-sass -o public/root src/server/root/root.scss",
    "deploy": "platform-deploy qtip production",
    "sass:watch": "npm run sass && npm run sass -- --watch --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4shub/qtip.git"
  },
  "author": "Shubham Naik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/4shub/qtip/issues"
  },
  "homepage": "https://github.com/4shub/qtip#readme",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/mongodb": "^3.5.14",
    "@types/multer": "^1.4.3",
    "@types/multer-s3": "^2.7.7",
    "@types/node": "^13.13.4",
    "@types/showdown": "^1.9.3",
    "babel-preset-env": "^1.7.0",
    "node-sass": "^4.14.0",
    "nodemon": "^2.0.3",
    "parcel": "^1.12.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "aws-sdk": "^2.668.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "handlebars": "^4.7.6",
    "mongodb": "^3.5.6",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "showdown": "^1.9.1"
  }
}
